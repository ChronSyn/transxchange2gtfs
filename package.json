  "devDependencies": {
    "@types/autobind-decorator": "^2.1.0",
    "@types/chai": "^4.1.5",
    "@types/csv-parse": "^1.1.11",
    "@types/glob": "^7.1.0",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.10.3",
    "@types/rimraf": "^2.0.2",
    "@types/xml2js": "^0.4.3",
    "chai": "^4.1.2",
    "mocha": "^5.2.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.0.3"
  },
  "dependencies": {
    "autobind-decorator": "^2.1.0",
